# Alias para abrir Chrome com Charles Proxy
alias chrome-charles='/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --user-data-dir="/tmp/chrome-charles" --proxy-server="127.0.0.1:8888" --ignore-certificate-errors --allow-insecure-localhost'
alias reload='source ~/.zshrc'

